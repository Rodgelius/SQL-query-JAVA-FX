<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
      <RowConstraints />
    </rowConstraints>
    <children>
          <TableView fx:id="tableHosts" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="1024.0" GridPane.rowIndex="1">
              <columns>
                  <TableColumn fx:id="row1" minWidth="100.0" prefWidth="-1.0" text="id" />
                  <TableColumn fx:id="row2" minWidth="100.0" prefWidth="-1.0" text="event_time" />
                  <TableColumn fx:id="row3" minWidth="100.0" prefWidth="-1.0" text="trigger" />
            <TableColumn fx:id="row4" minWidth="100.0" prefWidth="-1.0" text="item" />
            <TableColumn fx:id="row5" minWidth="100.0" prefWidth="-1.0" text="status" />
            <TableColumn fx:id="row6" minWidth="100.0" prefWidth="-1.0" text="priority" />
            <TableColumn fx:id="row7" minWidth="100.0" prefWidth="-1.0" text="name" />
            <TableColumn fx:id="row8" minWidth="100.0" prefWidth="-1.0" text="ip" />
              </columns>
          </TableView>
      <Button fx:id="btn" contentDisplay="CENTER" layoutX="500.0" layoutY="500.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Search">
         <GridPane.margin>
            <Insets bottom="5.0" left="410.0" top="5.0" />
         </GridPane.margin></Button>
      <TextField fx:id="textfield" maxHeight="26.0" maxWidth="400.0" minHeight="26.0" minWidth="204.0" prefHeight="26.0" prefWidth="204.0" scaleShape="false" text="Enter hostname here and press Search">
         <GridPane.margin>
            <Insets left="5.0" />
         </GridPane.margin></TextField>
    </children>
</GridPane>
